{
  "name": "just-for-today",
  "version": "1.0.3",
  "scripts": {
    "start": "expo start --dev-client",
    "start-tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "format": "npx prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "format:fix": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "clean": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\" && npm run lint:fix",
    "test": "jest -u",
    "update": "eas update --branch develop"
  },
  "dependencies": {
    "@react-native-community/masked-view": "^0.1.11",
    "@types/react": "~18.2.37",
    "date-fns": "^2.30.0",
    "expo-asset": "~8.10.1",
    "expo-dev-client": "~2.4.12",
    "expo-font": "~11.6.0",
    "expo-linking": "~6.0.0",
    "expo-router": "^2.0.12",
    "expo-splash-screen": "~0.22.0",
    "expo-status-bar": "~1.7.1",
    "expo-updates": "~0.18.19",
    "expo-web-browser": "~12.5.0",
    "expo": "^49.0.18",
    "react-dom": "18.2.0",
    "react-native-gesture-handler": "~2.13.4",
    "react-native-reanimated": "~3.5.4",
    "react-native-safe-area-context": "4.7.4",
    "react-native-screens": "~3.27.0",
    "react-native-web": "~0.19.9",
    "react-native": "0.72.7",
    "react-redux": "^8.1.3",
    "react": "18.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@expo/vector-icons": "^13.0.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/native": "^6.1.9",
    "@reduxjs/toolkit": "^1.9.7",
    "@testing-library/react-native": "^12.4.0",
    "@types/jest": "^29.5.8",
    "@types/react-native": "~0.72.6",
    "@types/react-redux": "^7.1.30",
    "@types/react-test-renderer": "^18.0.6",
    "@types/react": "~18.2.37",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@wildpastry/eslint-config": "^1.6.4",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint": "^8.53.0",
    "expo-dev-client": "~2.4.12",
    "hermes-engine": "^0.11.0",
    "jest-expo": "~49.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
