{
  "name": "just-for-today",
  "version": "1.2.0",
  "main": "./app/App.tsx",
  "scripts": {
    "build-develop:android": "eas build --profile develop --platform android",
    "build-develop:ios": "eas build --profile develop --platform ios",
    "build-develop": "eas build --profile develop --platform all",
    "build-prod:android": "eas build --profile production --platform android",
    "build-prod:ios": "eas build --profile production --platform ios",
    "build-prod": "eas build --profile production --platform all",
    "clean": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\" && npm run lint:fix",
    "format:fix": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "format": "npx prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "start-tunnel": "expo start --tunnel",
    "start": "expo start --dev-client",
    "submit:android": "eas submit -p android",
    "submit:ios": "eas submit -p ios",
    "test": "jest -u",
    "update": "eas update --branch develop"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.2",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "date-fns": "^3.6.0",
    "expo": "~51.0.8",
    "expo-linking": "^6.3.1",
    "expo-router": "^3.5.14",
    "expo-splash-screen": "^0.27.4",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "^0.25.14",
    "expo-web-browser": "^13.0.3",
    "jest": "^29.7.0",
    "react": "18.3.1",
    "react-native": "0.74.1",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-safe-area-context": "^4.10.1",
    "react-native-screens": "^3.31.1",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.24.5",
    "@react-navigation/bottom-tabs": "^6.5.20",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.5",
    "@testing-library/react-native": "^12.5.0",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.3.2",
    "@types/react-native": "^0.73.0",
    "@types/react-redux": "^7.1.33",
    "@types/react-test-renderer": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.9.0",
    "@wildpastry/eslint-config": "^1.6.5",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "expo-dev-client": "^4.0.14",
    "jest-expo": "^51.0.2",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "private": true
}
