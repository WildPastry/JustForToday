{
  "name": "just-for-today",
  "version": "1.0.3",
  "main": "./app/App.tsx",
  "scripts": {
    "android": "expo run:android",
    "build-production": "eas build --profile production --platform android",
    "build-staging": "eas build --profile preview --platform android",
    "build-develop": "eas build --profile develop --platform android",
    "clean": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\" && npm run lint:fix",
    "format:fix": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "format": "npx prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "ios": "expo run:ios",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "start-tunnel": "expo start --tunnel",
    "start": "expo start --dev-client",
    "submit": "eas submit -p android",
    "test": "jest -u",
    "update": "eas update --branch develop",
    "web": "expo start --web"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-masked-view/masked-view": "^0.3.1",
    "date-fns": "^3.4.0",
    "expo": "~50.0.11",
    "expo-linking": "^6.2.2",
    "expo-router": "^3.4.8",
    "expo-splash-screen": "^0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "^0.24.11",
    "expo-web-browser": "^12.8.2",
    "jest": "^29.7.0",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "^2.15.0",
    "react-native-safe-area-context": "^4.9.0",
    "react-native-screens": "^3.29.0",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@react-navigation/bottom-tabs": "^6.5.18",
    "@react-navigation/native": "^6.1.15",
    "@react-navigation/native-stack": "^6.9.24",
    "@reduxjs/toolkit": "^2.2.1",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/react": "~18.2.65",
    "@types/react-native": "^0.73.0",
    "@types/react-redux": "^7.1.33",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@wildpastry/eslint-config": "^1.6.4",
    "eslint": "^8.57.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "expo-dev-client": "^3.3.9",
    "jest-expo": "^50.0.3",
    "prettier": "^3.2.5",
    "react-test-renderer": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2"
  },
  "private": true
}
