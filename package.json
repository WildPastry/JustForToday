{
  "name": "just-for-today",
  "version": "1.0.3",
  "scripts": {
    "start": "npx expo start",
    "start-dev": "npx expo start --dev-client",
    "start-tunnel": "npx expo start --tunnel",
    "start-android": "npx expo start --android",
    "start-ios": "npx expo start --ios",
    "start-web": "npx expo start --web",
    "format": "npx prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "format:fix": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "clean": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\" && npm run lint:fix",
    "test": "jest -u",
    "update": "eas update --branch develop"
  },
  "dependencies": {
    "date-fns": "^2.30.0",
    "expo-asset": "~8.12.0",
    "expo-dev-client": "~2.4.6",
    "expo-font": "~11.6.0",
    "expo-linking": "~6.0.0",
    "expo-router": "2.0.6",
    "expo-splash-screen": "~0.22.0",
    "expo-status-bar": "~1.7.1",
    "expo-updates": "~0.18.12",
    "expo-web-browser": "~12.5.0",
    "expo": "^49.0.10",
    "react-dom": "18.2.0",
    "react-native-safe-area-context": "4.7.2",
    "react-native-screens": "~3.25.0",
    "react-native-web": "^0.19.8",
    "react-native": "0.72.4",
    "react-redux": "^8.1.2",
    "react": "18.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native-stack": "^6.9.13",
    "@react-navigation/native": "^6.1.7",
    "@reduxjs/toolkit": "^1.9.5",
    "@testing-library/react-native": "^12.3.0",
    "@types/jest": "^29.5.4",
    "@types/react-native": "~0.72.2",
    "@types/react-redux": "^7.1.26",
    "@types/react-test-renderer": "^18.0.1",
    "@types/react": "~18.2.21",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@wildpastry/eslint-config": "^1.6.4",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint": "^8.49.0",
    "expo-dev-client": "^2.4.8",
    "hermes-engine": "^0.11.0",
    "jest-expo": "~49.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "react-test-renderer": "18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "private": true
}
