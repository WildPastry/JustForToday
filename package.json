{
  "name": "just-for-today",
  "version": "1.0.3",
  "main": "./app/App.tsx",
  "scripts": {
    "start": "expo start --dev-client",
    "start-tunnel": "expo start --tunnel",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "format": "npx prettier --check \"**/*.{js,jsx,ts,tsx}\"",
    "format:fix": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" --max-warnings 0",
    "lint:fix": "eslint --fix \"**/*.{js,jsx,ts,tsx}\"",
    "clean": "npx prettier --write \"**/*.{js,jsx,ts,tsx}\" && npm run lint:fix",
    "test": "jest -u",
    "update": "eas update --branch develop"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "date-fns": "^3.3.1",
    "expo-linking": "^6.2.2",
    "expo-router": "^3.4.6",
    "expo-splash-screen": "^0.26.4",
    "expo-status-bar": "~1.11.1",
    "expo-web-browser": "^12.8.2",
    "expo": "~50.0.4",
    "jest": "^29.7.0",
    "react-native": "0.73.2",
    "react-redux": "^9.1.0",
    "react": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/native": "^6.1.9",
    "@reduxjs/toolkit": "^2.1.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/react-native": "^0.73.0",
    "@types/react-redux": "^7.1.33",
    "@types/react-test-renderer": "^18.0.7",
    "@types/react": "~18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@wildpastry/eslint-config": "^1.6.4",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint": "^8.56.0",
    "expo-dev-client": "^3.3.7",
    "jest-expo": "^50.0.1",
    "prettier": "^3.2.4",
    "react-test-renderer": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.1.3"
  },
  "private": true
}
