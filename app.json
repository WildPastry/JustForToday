{
  "expo": {
    "name": "just-for-today",
    "slug": "justfortoday",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./app/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2c2c54"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/images/adaptive-icon.png",
        "backgroundColor": "#2c2c54"
      },
      "package": "nz.co.mikeparker.jft"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./app/assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "5a7e0b8b-ed87-4a04-8f49-770e0a9f0a4c"
      }
    },
    "owner": "wildpastry"
  }
}
